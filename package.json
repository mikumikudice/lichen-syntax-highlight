{
    "name": "lichen",
    "publisher": "mikaela md <mikomdias@yahoo.com>",
    "displayName": "lichen syntax highlight",
    "description": "basic code highlighting for the lichen programming language",
    "version": "0.0.1",
    "license": "BUNNYWARE",
    "engines": {
        "vscode": "^1.99.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "keywords": [
		"lichen", "moss", "llc", "mmc"
	],
    "contributes": {
        "languages": [{
            "id": "lichen",
            "aliases": ["lichen", "moss"],
            "extensions": [".lic", ".lim"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "lichen",
            "scopeName": "source.lichen",
            "path": "./lichen.tmLanguage.json"
        }]
    },
    "__metadata": {
		"publisherDisplayName": "mikaela morais dias",
		"targetPlatform": "universal",
		"isApplicationScoped": false,
		"updated": true,
		"isPreReleaseVersion": false,
		"hasPreReleaseVersion": false
	}
}
